{
  "name": "broteam-translate-bot",
  "private": true,
  "license": "WTFPL",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "debug": "ts-node-dev --respawn --transpile-only --inspect=9229 src/index.ts",
    "lint": "eslint src scripts --ext .ts,.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "translate:one": "ts-node src/scripts/translateOne.ts",
    "test:tokens": "ts-node src/scripts/testTokenizer.ts",
    "test:humor": "ts-node src/scripts/testHumorScorer.ts",
    "resolve:user": "ts-node src/scripts/resolveUserId.ts",
    "oauth2:auth": "ts-node src/scripts/oauth2Authorize.ts",
    "oauth2:handle": "ts-node src/scripts/oauth2HandleCode.ts",
    "oauth2:check": "ts-node src/scripts/checkOAuth2Health.ts",
    "admin": "ts-node src/scripts/adminCli.ts"
  },
  "dependencies": {
    "@huggingface/inference": "^4.13.5",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^17.2.3",
    "jsonrepair": "^3.13.2",
    "langdetect": "^0.2.1",
    "onnxruntime-node": "^1.23.2",
    "tinyld": "^1.3.4",
    "twitter-api-v2": "^1.14.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "pm2": "^6.0.14",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.test.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"]
  },
  "overrides": {
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.23"
  },
  "version": "0.9.0"
}
