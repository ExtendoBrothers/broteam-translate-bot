{
  "name": "broteam-translate-bot",
  "version": "0.6.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "debug": "ts-node-dev --respawn --transpile-only --inspect=9229 src/index.ts",
    "lint": "eslint . --ext .ts",
    "translate:one": "ts-node src/scripts/translateOne.ts",
    "test:tokens": "ts-node src/scripts/testTokenizer.ts",
    "resolve:user": "ts-node src/scripts/resolveUserId.ts",
    "oauth2:auth": "ts-node src/scripts/oauth2Authorize.ts",
    "oauth2:handle": "ts-node src/scripts/oauth2HandleCode.ts",
    "oauth2:check": "ts-node src/scripts/checkOAuth2Health.ts",
    "admin": "ts-node src/scripts/adminCli.ts"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "langdetect": "^0.2.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.6.7",
    "pm2": "^6.0.13",
    "tinyld": "^1.3.4",
    "twitter-api-v2": "^1.14.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^8.40.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "js-yaml": "^4.1.1"
  }
}
